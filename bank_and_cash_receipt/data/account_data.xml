<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="sequence_bkm" model="ir.sequence">
            <field name="name">Bukti Bank Keluar sequence</field>
            <field name="code">bukti.bank.keluar</field>
            <field name="prefix">BBK/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

